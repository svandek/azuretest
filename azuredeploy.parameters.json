{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "resourceName": {
            "value": "${{ secrets.AZURE_RG }}"
        },
        "location": {
            "value": "${{ secrets.AZURE_LOCATION }}"
        },
        "kubernetesVersion": {
            "value": "1.18.10"
        },
        "networkPlugin": {
            "value": "azure"
        },
        "enableRBAC": {
            "value": true
        },
        "enablePrivateCluster": {
            "value": false
        },
        "enableHttpApplicationRouting": {
            "value": false
        },
        "enableAzurePolicy": {
            "value": true
        },
        "vnetCidr":{
            "value": "10.0.0.0/8"
        },
        "subnetCidr":{
            "value": "10.240.0.0/16"
        },
        "serviceCidr": {
            "value": "10.0.0.0/16"
        },
        "dnsServiceIP": {
            "value": "10.0.0.10"
        },
        "dockerBridgeCidr": {
            "value": "172.17.0.1/16"
        },
        "networkPolicy": {
            "value": "azure"
        },
        "osDiskSizeLinux": {
            "value": 0
        },
        "nodeCountLinux":{
            "value": 1
        },
        "vmSizeLinux":{
            "value": "standard_b2s"
        },
        "enableDiagnostics":{
            "value": true
        },
        "omsSku":{
            "value": "free"
        },
        "adminGroupObjectIDs": {
            "value": []
        }      
    }
}